<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>7.1</LangVersion>
    <RepositoryUrl>https://github.com/Nazgul07/AnyDataType</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <Description>A datatype for strings, numerics, bool, and DateTime.

"Any" is a datatype capable of storing a number of different datatype values, and perform mathmatical and equality operations against other datatypes including "Any".</Description>
    <Copyright>Colton Correll</Copyright>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
